{
	"name": "hmmh/devlog",
	"type": "typo3-cms-extension",
	"description": "The Developer's Log extension provides development logging/debugging functionality and a BE module to browse the logs.",
	"license": [
		"GPL-2.0-or-later"
	],
	"authors": [
		{
			"name": "Francois Suter",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"Devlog\\Devlog\\": "Classes/"
		}
	},
	"require": {
		"typo3/cms-core": ">=9.5.0,<10.0"
	},
	"replace": {
		"devlog/devlog": "self.version",
		"typo3-ter/devlog": "self.version"
	}
}
